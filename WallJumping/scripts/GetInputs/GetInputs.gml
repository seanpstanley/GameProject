/// @desc Get Inputs
key_left = keyboard_check(ord("A")) || keyboard_check_direct(vk_left);
key_right = keyboard_check(ord("D")) || keyboard_check_direct(vk_right);
key_down = keyboard_check(ord("S")) || keyboard_check_direct(vk_down);
key_dash = keyboard_check_pressed(vk_lshift);
key_jump = keyboard_check_pressed(vk_space);
